          <argument>
            <kind>1</kind>
            <string>instance_create(0, 0, objGUI);

// input vars

k_left = 0;
k_right = 0;
k_up = 0;
k_down = 0;
k_action = 0;

// general vars

inWater = false;
wasInWater = false;
death = 0;
onBubble = false;
bubble = noone;
bubbleJumps = 0;
state = s.IDLE;
dir = global.spawn_dir;
xVel = 0;
yVel = 0;
yGrav = .15;
jumped = false;

// drawing vars

d_angle = 360;
d_xscale = sign(dir);
d_yscale = 1;
d_frame = 0;
d_sin = 0;
d_t = 0;
d_offx = 0;
d_offy = 0;
</string>
